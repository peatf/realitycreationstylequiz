{
  "framework": "nextjs",
  "buildCommand": "next build",
  "outputDirectory": ".next",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          key: 'Content-Security-Policy',
            value: "frame-ancestors 'self' https://alesha-legair-fnpz.squarespace.com https://www.peathefeary.com"
          }
      ]
    }
  ]
}
